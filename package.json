    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.4.1",
    "cors": "^2.8.5",
    "crypto-random-string": "^3.2.0",
    "express": "^4.17.3",
    "express-session": "^1.17.0",
    "express-validator": "^6.4.0",
    "if-env": "^1.0.4",
    "mysql": "^2.18.1",
